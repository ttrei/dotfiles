! Use RightWin as Ctrl
remove mod4 = Super_R
add control = Super_R

! Use CapsLock (66) key as Ctrl
keycode 66 = Control_L
clear lock
add control = Control_L

! Use Menu (135), PrintScreen (107) and ScrLock (78) keys as CapsLock
keycode 135 = Caps_Lock
keycode 107 = Caps_Lock
keycode 78 = Caps_Lock
add lock = Caps_Lock
